<?xml version="1.0"?><config>    <modules>        <Vacspare_Sintax>            <version>0.1.1</version>        </Vacspare_Sintax>    </modules>        <global>        <models>            <sintax>                <class>Vacspare_Sintax_Model</class>                <resourceModel>sintax_mysql4</resourceModel>            </sintax>            <sintax_mysql4>                <class>Vacspare_Sintax_Model_Mysql4</class>            </sintax_mysql4>        </models>        <helpers>            <sintax>                <class>Vacspare_Sintax_Helper</class>            </sintax>          </helpers>		<resources>            <sintax_setup>                <setup>                    <module>Vacspare_Sintax</module>                        <class>Vacspare_Sintax_Model_Resource_Eav_Mysql4_Setup</class>                                  </setup>                <connection>                    <use>core_setup</use>                </connection>            </sintax_setup>            <sintax_write>                <connection>                    <use>core_write</use>                </connection>            </sintax_write>            <sintax_read>                <connection>                    <use>core_read</use>                </connection>            </sintax_read>                    </resources>    </global>        <admin>        <routers>            <adminhtml>				<args>					<modules>						<sintax before="Mage_Adminhtml">Vacspare_Sintax_Adminhtml</sintax>					</modules>				</args>			</adminhtml>        </routers>    </admin>        <adminhtml>        <translate>            <modules>                <mage_adminhtml>                    <files>                        <sintax>Vacspare_Sintax.csv</sintax>                    </files>                </mage_adminhtml>            </modules>        </translate>                <menu>            <catalog>                <children>                    <sintax_adminform translate="title" module="sintax">                        <title>My Custom Form</title>                        <action>adminhtml/vacspare_sintax_myform</action>                    </sintax_adminform>                </children>            </catalog>        </menu>                <acl>            <resources>                <admin>                    <children>                        <catalog>                            <children>                                <sintax_adminform>                                    <title>My Form</title>                                </sintax_adminform>                            </children>                        </catalog>                    </children>                </admin>            </resources>        </acl>                <layout>            <updates>                <sintax>                    <file>sintax.xml</file>                </sintax>            </updates>        </layout>    </adminhtml></config>